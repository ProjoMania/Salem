<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="warning_wizard_view_form" model="ir.ui.view">
            <field name="name">warning.wizard.view.form</field>
            <field name="model">warning.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                            <field name="msg" nolabel="1"/>
                        <footer>
                            <button string="Ok" type="object" class="btn btn-primary" name="continue_process"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>
