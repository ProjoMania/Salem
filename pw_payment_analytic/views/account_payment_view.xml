<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_payment_form_inherit_pw_anaytic" model="ir.ui.view">
        <field name="name">account.payment.form.inherit.pw.analytic</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_bank_id']" position="after">
                <field name="analytic_account_id" groups="pw_payment_analytic.pw_group_analytic_account" readonly="state != 'draft'"/>
                <field name="analytic_tag_ids" widget="many2many_tags" groups="pw_payment_analytic.pw_group_analytic_tags" readonly="state != 'draft'"/>
            </xpath>
        </field>
    </record>

