<?xml version="1.0" encoding="utf-8"?>
<odoo>
         <record id="view_partner_form_extend" model="ir.ui.view">
            <field name="name">res.partner.property.form.extended</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='property_stock_supplier']" position="after">
                    <field name="liquidation_id" domain="[('is_liquidation','=',True)]"/>
                </xpath>
            </field>
         </record>
</odoo>