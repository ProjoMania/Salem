<odoo>
    <data>

        <!-- Form View -->
        <record model="ir.ui.view" id="form_view_competitors_images">
            <field name="name">Competitors Images Form View</field>
            <field name="model">competitors.images</field>
            <field name="arch" type="xml">
                <form string="Competitors Image Form">
                    <sheet>
                        <group>
                            <field name="image" widget="image"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- List view -->
        <record model="ir.ui.view" id="list_view_competitors_images">
            <field name="name">Competitors Images List View</field>
            <field name="model">competitors.images</field>
            <field name="arch" type="xml">
                <tree string="Competitors List">
                    <field name="image"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>