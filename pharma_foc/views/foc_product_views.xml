<odoo>
    <data>
    
        <!-- Tree View -->
        <record model="ir.ui.view" id="pharma_foc.foc_product_tree_view">
            <field name="name">FOC Product Tree View</field>
            <field name="model">foc.product</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="product_id"/>
                    <field name="percentage"/>
                    <button name="add_product" string="ADD" type="object" class="btn btn-primary"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
