        <record id="studio_custom_odoo_studio_purchase_d172b903-17fb-44c9-b86c-c64480dc0fae" model="ir.ui.view">
            <field name="name">studio.custom.odoo.studio.purchase.d172b903-17fb-44c9-b86c-c64480dc0fae</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
