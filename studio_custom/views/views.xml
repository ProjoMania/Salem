            </field>
        </record>
        <record id="studio_custom_odoo_studio_res_comp_e778267c-7030-4aa1-b175-2d86f323cfbf" model="ir.ui.view">
            <field name="name">studio.custom.odoo.studio.res.comp.e778267c-7030-4aa1-b175-2d86f323cfbf</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
