                <xpath expr="//form[1]/sheet[1]//field[@name='amount']" position="before">
                    <field name="x_studio_field_Gb6C2"/>
                </xpath>
                <xpath expr="//field[@name='journal_id']" position="after">
                    <field name="x_studio_payment_type_" string="طريقة السداد"/>
