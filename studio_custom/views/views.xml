                <xpath expr="//field[@name='website']" position="after">
                    <field name="x_studio_background_report" options="{'size':[0,270]}" widget="image"
                           string="Background Report"/>
                </xpath>


            </field>
        </record>


        <record id="studio_custom_odoo_studio_hr_appli_99b1a0ac-b5d0-4dc4-bfe7-b8874b967d90" model="ir.ui.view">
            <field name="name">studio.custom.odoo.studio.hr.appli.99b1a0ac-b5d0-4dc4-bfe7-b8874b967d90</field>
            <field name="model">hr.applicant</field>
            <field name="inherit_id" ref="hr_recruitment.hr_applicant_view_search_bis"/>
            <field name="arch" type="xml">


                <xpath expr="//field[@name='partner_name']" position="before">
                    <field name="x_studio_years_or_experience"/>
                </xpath>
                <xpath expr="//field[@name='email_from']" position="after">
                    <field name="x_studio_age"/>
                </xpath>
                <xpath expr="//filter[@name='stage']" position="after">
                    <filter name="studio_group_by_R9wmL" string="Field of Study"
                            context="{'group_by': 'x_studio_field_of_study_1'}"/>
                   <!-- <filter name="studio_group_by_suSDo" string="City" context="{'group_by': 'city'}"/> -->
                </xpath>


            </field>
        </record>


        <record id="studio_custom_odoo_studio_jobs_rec_b7456cd3-a029-41ab-8585-5cedbbaf1eb6" model="ir.ui.view">
            <field name="name">studio.custom.odoo.studio.jobs.rec.b7456cd3-a029-41ab-8585-5cedbbaf1eb6</field>
            <field name="model">hr.applicant</field>
            <field name="inherit_id" ref="hr_recruitment.hr_applicant_view_form"/>
            <field name="arch" type="xml">


                <xpath expr="//field[@name='type_id']" position="after">
                    <field widget="image" options="{'size': [0, 90]}" name="x_studio_photo" string="Photo"/>
                    <field name="x_studio_age" string="Age" widget="monetary" digits="1,0"/>
                    <field name="x_studio_years_or_experience" string="Years or Experience "/>
                    <field name="x_studio_field_of_study_1" string="Field of Study"/>
                    <field name="x_studio_city" string="City"/>
                </xpath>


            </field>
        </record>


