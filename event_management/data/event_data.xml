        <record id="event_attachment_attendees_list" model="event.attachment">
            <field name="name">Attendees List</field>
        </record>

        <record id="budget_name_venue_rental" model="budget.name">
            <field name="name">Venue Rental</field>
        </record>

        <record id="budget_name_catering_food" model="budget.name">
            <field name="name">Catering And Food</field>
        </record>

        <record id="budget_name_market_advert" model="budget.name">
