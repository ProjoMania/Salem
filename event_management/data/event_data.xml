        </record>

        <record id="event_attachment_opening_script" model="event.attachment">
            <field name="name">Opening Script</field>
        </record>

        <record id="event_attachment_invitation" model="event.attachment">
            <field name="name">Invitation</field>
        </record>

        <record id="event_attachment_invoice" model="event.attachment">
