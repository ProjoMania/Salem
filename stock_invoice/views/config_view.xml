<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_payment_type_tree" model="ir.ui.view">
            <field name="name">payment.type.tree</field>
            <field name="model">payment.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_payment_type_form" model="ir.ui.view">
            <field name="name">view.payment.type.form</field>
            <field name="model">payment.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_payment_type_act_window" model="ir.actions.act_window">
            <field name="type">ir.actions.act_window</field>
            <field name="name">Payment Type</field>
            <field name="res_model">payment.type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to add new payment type.
                </p>
            </field>
        </record>

